        <div class="container text-center text-md-left">
          <!-- Grid row -->
          <div class="row">
            <!-- Grid column -->
            <div class="col-md-2 mx-auto">
              <!-- Content -->
              <h5 class="font-weight-bold text-uppercase mt-3 mb-4">Picarousel</h5>
            </div>
            <!-- Grid column -->
          </div>
          <!-- Grid row -->
        </div>
        <!-- Footer Links -->
        <hr>
        <!-- Registration button -->
        <ul class="list-unstyled list-inline text-center py-2">
          <li class="list-inline-item">
            <h5 class="mb-1" href="">Register for free</h5>
          </li>
