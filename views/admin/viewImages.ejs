    <div id="images" class="tab-pane fade in active">
      <h3>IMAGES</h3>
      <table id="imagesTable" class="table table-striped">
          <thead>
            <tr>
              <th>ID</th>
              <th>Image</th>
              <th>Category</th>
              <th>Description</th>
              <th>Owner</th>
              <th>Verification Status</th>
              <th>License Type</th>
              <th>Privacy</th>
              <th>User Deleted</th>
              <th></th>
            </tr>
          </thead>
          <tbody>

  


 <% for (var i=0; i<result.length; i++) {%>
 <%console.log(result[i].filepath);%>
            <tr id = "photo<%=result[i].photo_id %>">
              <td>
                <%= result[i].photo_id %>
              </td>
              <td>

                <a target="_blank" onclick="makeCategorieEditable(<%=result[i].photo_id %>,'<%=result[i].photo_id %>');"  href="<%=result[i].filepath %>">
                <img src="<%=result[i].filepath %>" alt="Image" style="width:150px">
                </a>
              </td>
              <td>
                <%= result[i].photo_categorie %>
              </td>
              <td>
                <%= result[i].description %>
              </td>
              <td>
                <%= result[i].owner_user_id %>
              </td>
               <td>
                  <%= result[i].adminverified %>
              </td>
              <td>
                  <%= result[i].licencetype %>
              </td>
              <td>
                  <%= result[i].privacy %>
              </td>
              <td>
              <% if (result[i].deleted == 0){%>
                  live
              <% }else{%>
                  deleted
              <%}%>
              </td>
              <td>
                  <input name="imageCheckboxids[]" type="checkbox" class="form-check-input" value="<%=result[i].photo_id %>">
                
              </td>
            </tr>

 <%}%>
            <tr><td></td><td></td><td></td><td></td><td></td><td colspan="4">Set Verification Status:
              
                <button style="color:white;background-color:#d65151;" type="button" onclick="postStatusImage('waitingImage');" class="btn btn-secondary btn-sm">Waiting </button>
                <button style="color:white;background-color:#d65151;" type="button" onclick="postStatusImage('verifiedImage');" class="btn btn-secondary btn-sm">Verified</button>
                 <button style="color:white;background-color:#d65151;" type="button" onclick="postStatusImage('rejectedImage');" class="btn btn-secondary btn-sm">Rejected </button>
              </td>
            </tr>
           
          </tbody>
      </table>
      <% if ( typeof page != 'undefined' ) { %>


            <a href="#" onclick="navigateToPage('imagesChangePage','categorieContent','-');" class="first">&laquo; Prev</a>
            <%var k=0;%>
            <% for ( k=1; k<=totalpage; k++) { %>
            <%if (page !=k){%>
              <a href="#" class="next round " onclick="navigateToPage('imagesChangePage','categorieContent','<%= k%>')"> <%= k%> </a>
            <%}else{%>
              <a href="#" class="next round " style="background-color:#d65151;" onclick="navigateToPage('imagesChangePage','categorieContent','<%= k%>')"> <%= k%> </a>
            <%}%>
            <%}%>
            <a href="#" onclick="navigateToPage('imagesChangePage','categorieContent','+');"class="last">Next &raquo;</a>
      <% } %>

